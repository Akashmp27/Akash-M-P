#include <stdio.h>
int main()
{
    int i, j;
    int matrix[3][3];

    /* Input data in matrix */
    for (i = 0; i < 3; i++)
    {
        for (j = 0; j < 3; j++)
        {
printf("element-[%d],[%d]: ", i,j);
            scanf("%d", &matrix[i][j]);
   
} 
  printf("\n");
    }

    /* Display the matrix */
    for (i = 0; i < 3; i++)
    {
        for (j = 0; j < 3; j++)
        {
            printf("%d\t", matrix[i][j]);
        }
        printf("\n");
    }

    return 0;
}
